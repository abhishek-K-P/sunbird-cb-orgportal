{
  "name": "mdo",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "export NODE_OPTIONS=--openssl-legacy-provider && node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng serve --proxy-config proxy/localhost.proxy.json -o",
    "dev": "export NODE_OPTIONS=--openssl-legacy-provider && npm run start:mdo-dev",
    "build-dev": "export NODE_OPTIONS=--openssl-legacy-provider &&  node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration production --configuration=dev --output-path=dist/www/en --base-href=/ --subresource-integrity --optimization=false --aot=false --build-optimizer=false",
    "build-benchmark": "export NODE_OPTIONS=--openssl-legacy-provider && node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration production --configuration=benchmark --output-path=dist/www/en --base-href=/ --subresource-integrity --optimization=false --aot=false --build-optimizer=false",
    "build-preprod": "export NODE_OPTIONS=--openssl-legacy-provider &&  node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration production --configuration=preprod --output-path=dist/www/en --base-href=/ --subresource-integrity --optimization=false --aot=false --build-optimizer=false",
    "build-np": "export NODE_OPTIONS=--openssl-legacy-provider &&  node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration production --configuration=np --output-path=dist/www/en --base-href=/ --subresource-integrity --optimization=false --aot=false --build-optimizer=false",
    "build": "export NODE_OPTIONS=--openssl-legacy-provider && node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration production --output-path=dist/www/en --base-href=/ --subresource-integrity --optimization=false --aot=false --build-optimizer=false",
    "postbuild": "export NODE_OPTIONS=--openssl-legacy-provider && npm run compress:gzip && npm run compress:brotli",
    "compress:brotli": "gzipper --brotli --gzip-level=9 --verbose ./dist/www",
    "compress:gzip": "gzipper --verbose --gzip-level=9 ./dist/www",
    "build:local": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration production",
    "build:stats": "ng build --stats-json",
    "start:mdo-dev": "node --max_old_space_size=12288 ./node_modules/@angular/cli/bin/ng serve --proxy-config proxy/mdo-dev-ip.proxy.json -o",
    "analyse": "webpack-bundle-analyzer ./dist/www/ng serve --proxy-config proxy/localhost.proxy.json -o/stats-es5.json",
    "test": "jest",
    "test-coverage": "./node_modules/jest/bin/jest.js --clearCache && node --max_old_space_size=1024 ./node_modules/jest/bin/jest.js --silent --ci --collectCoverage=true --coverageReporters=lcov --detectOpenHandles --forceExit=true",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "i18n": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng xi18n --output-path locale",
    "tailwind": "./node_modules/.bin/tailwind build ./src/tailwind-build.scss  -o ./src/styles.scss",
    "prestart": "npm run tailwind",
    "sonar": "sonar-scanner"
  },
  "private": true,
